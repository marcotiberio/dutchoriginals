<div class="flyntComponent boxed" is="flynt-block-wysiwyg-two-col">
  <div class="container">
    {% if title %}
      <div class="preContent">
        <div class="title h3">{{ title }}</div>
      </div>
    {% endif %}
    <div class="content">
      <div class="content-left">
        {{ contentHtmlLeft|e('wp_kses_custom') }}
      </div>
      <div class="content-right">
        {{ contentHtmlRight|e('wp_kses_custom') }}
      </div>
    </div>
  </div>
</div>
